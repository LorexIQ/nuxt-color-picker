{
  "name": "nuxt-color-picker",
  "version": "1.1.1",
  "keywords": [
    "vue",
    "nuxt",
    "nuxt3",
    "nuxt-module",
    "component",
    "style-kit",
    "color"
  ],
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/LorexIQ/nuxt-color-picker.git"
  },
  "author": {
    "name": "LorexIQ",
    "email": "dmitrymurashko0@gmail.com",
